//For loop test

print "Should be 1, 2, 3, 4, 5";
for (var i = 0; i < 5; i = i + 1) {
    print i;
}

print "Should be 5, 4, 3, 2, 1";
for (var i = 5; i > 0; i = i - 1) {
    print i;
}

//nested for loop test
print "Should print 'Hej' 6 times";
for (var i = 0; i < 2; i = i + 1) {
    for (var j = 0; j < 3; j = j + 1) {
        print "Hej";
    }
}